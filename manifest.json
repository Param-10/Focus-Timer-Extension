{
    "manifest_version": 3,
    "name": "Mindful Focus Extension",
    "version": "1.0",
    "description": "A Chrome extension for mindful browsing and focus time with gamification.",
    "permissions": ["storage", "notifications", "alarms"],
    "action": {
      "default_popup": "popup/popup.html",
      "default_icon": {
        "16": "images/icon16.svg",
        "48": "images/icon48.svg",
        "128": "images/icon128.svg"
      }
    },
  "background": {
    "service_worker": "background-wrap.js"
},
    "options_page": "options/options.html",
    "icons": {
      "16": "images/icon16.svg",
      "48": "images/icon48.svg",
      "128": "images/icon128.svg"
    }
  }